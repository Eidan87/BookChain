// En tu función generarDatosAleatorios(), ajustas el contenidoLibro con contenido generado
function generarDatosAleatorios() {
    const timestamp = Date.now();
    const transactions = [];
    const tituloLibro = `Libro_${timestamp}`;
    const autorLibro = `Autor_${timestamp}`;
    const contenidoLibro = generarContenidoExtenso(1000); // Genera 10 párrafos de Lorem Ipsum
    const cantidad = Math.floor(Math.random() * 100000) + 1;

    return new Block(
        cutreCoin.contarEslabones(),
        timestamp,
        transactions,
        tituloLibro,
        autorLibro,
        contenidoLibro,
        cantidad,
        cutreCoin.getUltimoBloque().hash,
        '',
        "uploads/imagenManifiesto.jpg"
    );
}

// Función para generar múltiples bloques
function generarBloques(cantidad) {
    for (let i = 0; i < cantidad; i++) {
        
        setTimeout(function() {
        const nuevoBloque = generarDatosAleatorios();
        cutreCoin.agregarBloque(nuevoBloque);
          }, 10000);
    }
}

// Generar 1000 bloques de ejemplo (ajustar según necesidad)
generarBloques(10);

console.log('Generación de bloques completada.');

